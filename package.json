  "engines": {
    "node": ">= 12.x",
    "npm": ">= 6.4.1",
    "yarn": ">= 1.19.1"
  },
  "dependencies": {
    "@mycrypto/ui": "1.0.6",
    "@testing-library/react-hooks": "7.0.1",
    "@types/react-slick": "0.23.5",
    "axios": "0.21.1",
    "gatsby": "3.10.1",
